@model WebApplication1.Models.Usuario

@{
    ViewData["Title"] = "Edit";
}


<h1>Editar Usuario</h1>

<h4>Usuario</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="UsuarioId" />
            <div class="accept-policy">
                <label asp-for="Nombre" class="control-label"></label>
                <input asp-for="Nombre" class="form-control" />
                <span asp-validation-for="Nombre" class="text-danger"></span>
            </div>
            <div class="accept-policy">
                <label asp-for="FechaRegistro" class="control-label"></label>
                <input asp-for="FechaRegistro" class="form-control" />
                <span asp-validation-for="FechaRegistro" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="RolId" class="control-label"></label>
                <select asp-for="RolId" class="form-control" asp-items="ViewBag.Roles"></select>
                <span asp-validation-for="RolId" class="text-danger"></span>
            </div>
            <div class="accept-policy">
                <label asp-for="EmailPrincipal" class="control-label"></label>
                <input asp-for="EmailPrincipal" class="form-control" />
                <span asp-validation-for="EmailPrincipal" class="text-danger"></span>
            </div>
        </form>
    </div>
</div>