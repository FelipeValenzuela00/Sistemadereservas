@model List<Usuario>

 <h1>Usuarios</h1>

 <table class="table">
     <thead>
         <tr>
             <th>Nombre</th>
            <th>Email</th>
         </tr>
         
             
         
     </thead>
     <tbody>
        @foreach (var usuario in Model)
        {
            <tr>
                <td>@Html.DisplayFor(e=>usuario.Nombre)</td>

                <td>@Html.DisplayFor(e => usuario.EmailPrincipal)</td>
            </tr>
           
               
            
        }
    </tbody>

 </table>